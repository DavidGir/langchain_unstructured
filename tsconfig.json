{
  "extends": "@tsconfig/node16/tsconfig.json",
  "compilerOptions": {
    "target": "es2020",    
    "module": "ESNext",    
    "esModuleInterop": true,
    "strict": true,
    "moduleResolution": "node", 
    "outDir": "./dist",    
    "baseUrl": ".",         
    "paths": {              
      "*": ["node_modules/*", "src/*"]
    },
    "allowSyntheticDefaultImports": true, 
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "./src/**/*",
    "./unstructured-js-client.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}

